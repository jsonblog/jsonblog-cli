{
  "name": "jsonblog-cli",
  "version": "2.0.0",
  "description": "A CLI tool to generate static blogs from JSON files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "dev": "ts-node src/index.ts serve"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "jsonblog-generator-boilerplate": "1.3.0",
    "jsonblog-schema": "0.0.7",
    "chokidar": "^3.5.3",
    "chalk": "^4.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "bin": {
    "blog": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
