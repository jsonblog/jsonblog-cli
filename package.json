{
  "name": "jsonblog-cli",
  "version": "1.0.0",
  "description": "CLI tool for JsonBlog",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jsonblog": "./dist/index.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "dev": "ts-node src/index.ts serve",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch -m \"Release %s\"",
    "release:minor": "npm version minor -m \"Release %s\"",
    "release:major": "npm version major -m \"Release %s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsonblog/jsonblog-cli.git"
  },
  "keywords": [
    "jsonblog",
    "blog",
    "cli",
    "static-site",
    "markdown"
  ],
  "author": "JSON Blog Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsonblog/jsonblog-cli/issues"
  },
  "homepage": "https://github.com/jsonblog/jsonblog-cli#readme",
  "dependencies": {
    "commander": "^11.1.0",
    "jsonblog-generator-boilerplate": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.2"
  }
}
